{
  "name": "dispachito",
  "version": "0.1.0",
  "description": "Event dispatcher/emitter for javascript node and browser",
  "main": "./dist/dispachito.min.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development webpack",
    "dev:watch": "npm run dev --watch",
    "build": "NODE_ENV=production webpack",
    "dist": "npm run clean && tsc --outDir ./dist/lib && tsc -m es6 --outDir ./dist/lib-esm && npm run dev && npm run build"
  },
  "keywords": [
    "event emitter",
    "event dispatcher",
    "typescript",
    "despasito"
  ],
  "author": {
    "name": "alexhoma",
    "web": "https://github.com/alexhoma",
    "email": "alexcm.14@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^9.6.1",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  }
}
